    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>My App</title>
        <link rel="stylesheet" type="text/css" href="bootstrap/dist/css/bootstrap.css">

        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">



        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>

        <script src="jquery/dist/jquery.js"></script>
        <script src="bootstrap/dist/js/bootstrap.js"></script>


        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    </head>
    <body>

        <!--[if lt IE 7]>
            <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->


        <!-- Add your site or application content here -->

        <div class="container body_div">



        <div class="container">
            <h1><%= title %></h1>
        </div>





        <div class="container">
            <h1 class="lead"><%= list_title %></h1>
        </div>





        <div class="container">
            <ul>
                <% for (var i = 0; i < items.length; i++) { %>
                    <li><%= items[i] %></li>
                <% } %>
            </ul>
        </div>


        <div class="container">



            <table class="table">
              <thead>
                <tr>
                  <th>#</th>

                  <th>First Name</th>
                  <th>Last Name</th>
                  <th>Reason</th>
                </tr>
                </thead>

            <tbody>

                <% for (var i = 0; i < presentResidentsList.length; i++) { %>
                    <tr>
                    <th scope="row"><%= i %></th>
                    <td><%= presentResidentsList[i].firstName %></td>
                    <td><%= presentResidentsList[i].lastName %></td>
                    <td><%= presentResidentsList[i].reasonNotPresent %></td>
                    </tr>
                <% } %>


                </tbody>

            </table>


        </div>








        <div class="container">
            <form action="/add" method="post">
                <input type="text" name="newItem" placeholder="Enter ID Number" autocomplete="off" autofocus>
                <input class="btn btn-default" type="submit" value="Submit">
            </form>
        </div>








        </div>

















    </body>


</html>